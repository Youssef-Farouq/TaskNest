{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*\\.css)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "text/css"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/assets/(.*\\.js)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}